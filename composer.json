{
    "name": "jeremynikolic/webinar-php",
    "description": "webinar-php",
    "keywords": [
        "jeremynikolic",
        "webinar-php"
    ],
    "homepage": "https://github.com/jeremynikolic/webinar-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Nikolic",
            "email": "nikolic.jeremy@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Jeremynikolic\\WebinarPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jeremynikolic\\WebinarPhp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
